<header class="sixteen wide column">
  <a id="js-back-to-main-centerbox-<%= character.id %>" href="#" class="grey">
    <i class="arrow left icon"></i><h3>Character Profile</h3>
  </a>
  <%= link_to summary_character_path(id: character.id, format: :pdf), class: 'ui button', style: "float: right", target: "_blank" do %>
    <small>PDF</small>
  <% end %>
  <%= link_to summary_character_path(character), class: 'ui button', style: "float: right", target: "_blank" do %>
    <small>Summary</small>
  <% end %>
</header>
