<%= simple_form_for @character do |f| %>
  <%= f.input :name %>
  <%= f.input :nationality %>
  <%= f.input :DOB, label: 'Date Of Birth', as: :date, start_year: Date.today.year - 50,
                    end_year: Date.today.year, discard_day: false,
                    order: [:day, :month, :year] %>
  <%= f.submit 'CREATE PERSONA' %>
<% end %>