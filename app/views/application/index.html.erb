<section class="dashboard_header">
  <section class="ui page grid">
    <h3>Dashboard</h3>
  </section>
</section>

<section class="ui page grid">
  <%= render 'application/profile' %>
  <%= render 'application/centerbox' %>
  <%= render 'application/create_character_modal' %>
  <%= render 'application/edit_profile_modal' %>
  <%= render 'application/create_space_modal' %>
</section>

<script>
  $(document).ready(function(){
    var character_id = location.search.split('character=')[1];
    $('#js-show-character-' + character_id).click();
    $('.ui.accordion')
      .accordion()
    ;
  })
</script>
